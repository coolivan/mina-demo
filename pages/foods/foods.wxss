/* pages/foods/foods.wxss */
.content{
  background: #f7f7f7;
}
.wrap .title{
  text-align: center;
  padding: 10px 0;
}
.list{
  display: flex;
  flex-wrap: wrap; 
  padding: 10rpx;
  /* justify-content: center; */
}
.list .item{
  width: 33.3%;
  box-sizing: border-box;
  padding-bottom: 30rpx;
  padding-left: 5rpx;
  padding-right: 5rpx;
}

.list .item image{
  max-width: 100%;
  height: 160rpx;
}
.list .item text{
  font-size: 32rpx;
  color: #666;
  text-align: center;
}